{
    "name": "tromsfylkestrafikk/laravel-siri",
    "description": "Handle incoming SIRI ET and VM in your Laravel project",
    "type": "library",
    "keywords": [
        "laravel",
        "siri",
        "routedata"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/console": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^1.2",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.23"
    },
    "autoload": {
        "psr-4": {
            "TromsFylkestrafikk\\Siri\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TromsFylkestrafikk\\Siri\\SiriServiceProvider"
            ]
        }
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "KÃ¥re Slettnes",
            "email": "kaare.slettnes@tffk.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
